{
  "compilerOptions": {
    "target": "es2018",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "module": "ESNext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "types": [
      "vitest",
      "vitest/globals",
      "@types/testing-library__jest-dom"
    ],
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": true,
    "removeComments": true,
    "importHelpers": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "src/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "./tsconfig.react.json"
    },
    {
      "path": "./tsconfig.setsunajs.json"
    }
  ],
}